<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jbcamera" android:versionCode="1" android:versionName="1.0">

    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-sdk android:targetSdkVersion="14" android:minSdkVersion="11"/>
    <application android:allowBackup="true" android:label="take picture">
        <activity android:name="com.example.jbcamera.MainActivity" android:configChanges="orientation|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>
</manifest>
